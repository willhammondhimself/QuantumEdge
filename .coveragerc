[run]
source = src
branch = True
parallel = True
omit =
    */tests/*
    */test_*
    */__pycache__/*
    */venv/*
    */env/*
    */.venv/*
    */virtualenv/*
    */node_modules/*
    */migrations/*
    */setup.py
    */conftest.py

[report]
precision = 2
show_missing = True
skip_covered = False
fail_under = 80
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover
    
    # Don't complain about missing debug-only code:
    def __repr__
    if self\.debug
    
    # Don't complain if tests don't hit defensive assertion code:
    raise AssertionError
    raise NotImplementedError
    
    # Don't complain if non-runnable code isn't run:
    if 0:
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod
    
    # Don't complain about type checking imports
    from typing import TYPE_CHECKING
    if TYPE_CHECKING:

[html]
directory = htmlcov

[xml]
output = coverage.xml

[json]
output = coverage.json